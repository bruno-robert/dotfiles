{
    "todo-tree.tree.showScanModeButton": false,
    "gitlens.advanced.telemetry.enabled": false,
    "terminal.integrated.fontFamily": "Inconsolata-dz for Powerline, dz",
    "psi-header.variables": [
        ["authoremail", "bruno.robert@gurussolutions.com"]
    ],
    "psi-header.lang-config": [
        {
            "language": "javascript",
            "begin": "/*\n***********************************************************************",
            "end": " ***********************************************************************/",
            "forceToTop": true
        }
    ],
    "psi-header.templates": [
        {
            "language": "javascript",
            "template": [
                "The following JavaScript code is created by GURUS Solutions,",
                "a NetSuite Partner. It is a SuiteFlex component containing custom code",
                "intended for NetSuite (www.netsuite.com) and uses the SuiteScript API.",
                "The code is provided \"as is\": GURUS Solutions shall not be liable",
                "for any damages arising out the intended use or if the code is modified",
                "after delivery.",
                "",
                "Company:     GURUS Solutions inc., www.gurussolutions.com",
                "             Cloud Consulting Pioneers",
                "Author:      <<authoremail>>",
                "Date:        <<filecreated('MMM, DD YYYY, h:mm:ss a')>>",
                "File:        <<filename>>"
            ]
        }
    ],
    "editor.minimap.enabled": false,
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "j"],
            "after": ["<Esc>"]
        }
    ],
    "vim.sneak": true,
    "vim.easymotion": true,
    "vim.handleKeys": { 
        "<C-f>": false,
        "<C-b>": false,
        "<C-j>": false,
        "<C-w>": false,
        "<C-c>": false,
        "<C-v>": false,
        "<C-a>": false
    },
    "todo-tree.general.tags": [
        "BUG:",
        "HACK:",
        "FIXME:",
        "TODO:",
        "XXX:",
        "INFO:",
        "NOTE:",
        "WARN:",
        "ERR:"
    ],
    "editor.cursorSurroundingLines": 10,
    "editor.lineNumbers": "relative",
    "window.menuBarVisibility": "toggle",
    "[javascript]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "editor.renderControlCharacters": false,
    "editor.renderWhitespace": "all",
    "window.zoomLevel": 1,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "todo-tree.highlights.customHighlight": {
        "TODO:": {
            "icon": "check",
            "type": "text-and-comment",
            "background": "purple",
            "foreground": "white",
            "iconColour": "purple",
        },
        "BUG:": {
            "icon": "bug",
            "background": "orange",
            "foreground": "black",
            "iconColour": "orange",
            "type": "text-and-comment",
        },
        "FIXME:": {
            "icon": "flame",
            "background": "orange",
            "foreground": "black",
            "iconColour": "orange",
            "type": "text-and-comment",
        },
        "WARN:": {
            "icon": "alert",
            "background": "orange",
            "foreground": "black",
            "iconColour": "orange",
            "type": "text-and-comment",
        },
        "NOTE:": {
            "icon": "clippy",
            "iconColour": "#f0d50a",
            "background": "#f0d50a",
            "foreground": "black",
            "type": "text-and-comment",
        },
        "INFO:": {
            "icon": "info",
            "iconColour": "yellow",
            "background": "yellow",
            "foreground": "black",
            "type": "text-and-comment",
        },
        "ERR:": {
            "icon": "zap",
            "background": "red",
            "foreground": "white",
            "iconColour": "red",
            "type": "text-and-comment",
        }
    },
    "explorer.confirmDragAndDrop": false,
    "cSpell.userWords": [
        "Concordia",
        "Netsuite"
    ],
    "[html]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "jupyter.askForKernelRestart": false,
    "jupyter.alwaysTrustNotebooks": true,
    "diffEditor.ignoreTrimWhitespace": true,
    "[css]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "[scss]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "workbench.editorAssociations": [
        {
            "viewType": "jupyter.notebook.ipynb",
            "filenamePattern": "*.ipynb"
        }
    ],
    "settingsSync.ignoredExtensions": [
        "idleberg.applescript",
        "ms-azuretools.vscode-docker",
        "msjsdiag.vscode-react-native",
        "rust-lang.rust",
        "johnstoncode.svn-scm",
        "tady.vscode-query-runner",
        "shinichi-takii.sql-bigquery",
        "silvenga.edi-x12-support",
        "dcortes92.freemarker",
        "geequlim.godot-tools",
        "dotjoshjohnson.xml",
        "grapecity.gc-excelviewer",
        "burkeholland.simple-react-snippets",
        "ericbirdsall.suitesnippets",
        "octref.vetur",
        "wordpresstoolbox.wordpress-toolbox",
        "denoland.vscode-deno"
    ],
    "python.languageServer": "Pylance",
    "remote.SSH.remotePlatform": {
        "192.168.1.101": "macOS"
    },
    "autoDocstring.docstringFormat": "sphinx",
    "window.autoDetectColorScheme": true,
    "python.showStartPage": false,
    "queryRunner.keyFilename": "/Volumes/GURUS_Solutions/service account keys/solid-terra-271714_query-runner-admin.json",
    "queryRunner.projectId": "solid-terra-271714",
    "[handlebars]": {
        "editor.defaultFormatter": "mblode.pretty-formatter"
    },
    "terminal.integrated.cursorStyle": "line",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "codestream.email": "bruno.jeanluke@icloud.com",
    "files.exclude": {
        "**/.git": false
    },
    "vim.vimrc.enable": true,
}